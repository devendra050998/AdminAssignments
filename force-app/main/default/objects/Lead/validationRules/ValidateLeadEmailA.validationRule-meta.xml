<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ValidateLeadEmailA</fullName>
    <active>true</active>
    <errorConditionFormula>IF( CONTAINS( MID( Email , 5, 7) , &quot;a&quot;)  ,true,false)</errorConditionFormula>
    <errorMessage>Remove &quot;a&quot;  from 5th  character to 12th character of Leads Email</errorMessage>
</ValidationRule>
